{
  "name": "base-express-api",
  "version": "1.0.0",
  "description": "Express TypeScript API Documentation",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "concurrently \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/app.js",
    "build": "tsc",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
} 